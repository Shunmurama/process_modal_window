    <%= csp_meta_tag %>

    <script src="https://kit.fontawesome.com/ac5b2f3156.js" crossorigin="anonymous"></script>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
